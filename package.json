{
  "name": "express-api-starter",
  "version": "2.0.0",
  "type": "module",
  "description": "A basic starter for an express.js API",
  "scripts": {
    "start": "NODE_ENV=production node src/index.js",
    "dev": "NODE_ENV=development node --watch src src/index.js",
    "lint": "eslint --fix src",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "helmet": "7.0.0",
    "morgan": "1.10.0"
  },
  "devDependencies": {
    "eslint": "8.45.0",
    "eslint-plugin-jest": "27.2.3",
    "jest": "29.6.1",
    "standard": "17.1.0",
    "supertest": "6.3.3"
  },
  "eslintConfig": {
    "extends": "./node_modules/standard/eslintrc.json",
    "rules": {
      "space-before-function-paren": "off"
    },
    "env": {
      "jest/globals": true
    },
    "plugins": [
      "jest"
    ]
  }
}